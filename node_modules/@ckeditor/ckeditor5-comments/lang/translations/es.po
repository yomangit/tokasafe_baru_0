msgid ""
msgstr ""
"Language: es\n"
"Plural-Forms: nplurals=2; plural=(n != 1);\n"
"Content-Type: text/plain; charset=UTF-8\n"

msgctxt "The label for the comment editor."
msgid "Comment editor"
msgstr "Editor de comentarios"

msgctxt "The label for the number of comments."
msgid "NUMBER_OF_COMMENTS"
msgid_plural "%0 Comments"
msgstr[0] "%0 comentario"
msgstr[1] "%0 comentarios"

msgctxt "The alert displayed for a comment that is too long."
msgid "TOO_LONG_COMMENT_ALERT"
msgstr "El contenido del comentario es demasiado extenso. Su comentario tiene %0 caracteres, pero el límite es de %1."

msgctxt "The placeholder for the reply field."
msgid "Reply..."
msgstr "Responder…"

msgctxt "The placeholder for the reply field when the comment thread is resolved."
msgid "Reply to reopen discussion..."
msgstr "Responder para reabrir la discusión..."

msgctxt "The placeholder for the new commend field."
msgid "Write a comment..."
msgstr "Escribir un comentario…"

msgctxt "The information that comment was made on element and has no context"
msgid "Comment was made on an element"
msgstr "Se hizo un comentario sobre un elemento."

msgctxt "The label for the button that enables editing a comment."
msgid "Edit"
msgstr "Editar"

msgctxt "The label for the button that resolves a comment thread."
msgid "Resolve"
msgstr "Resolver"

msgctxt "The label for the button that reopens a resolved comment thread."
msgid "Reopen"
msgstr "Reabrir"

msgctxt "The label for the button that deletes a comment."
msgid "Remove"
msgstr "Quitar"

msgctxt "The label for for the button that adds a reply to a comment thread."
msgid "Reply"
msgstr "Responder"

msgctxt "The confirmation message shown after pressing the delete comment button."
msgid "Delete comment?"
msgstr "¿Quiere eliminar el comentario?"

msgctxt "The confirmation message shown after pressing the delete comment thread button."
msgid "Delete comment thread?"
msgstr "¿Quiere eliminar el hilo de comentarios?"

msgctxt "A message informing that the comment has been resolved"
msgid "Marked as resolved"
msgstr "Se ha marcado como resuelto"

msgctxt "The tooltip for the comment toolbar button and the button that submits a comment thread."
msgid "Comment"
msgstr "Comentario"

msgctxt "The accessible label for the annotations sidebar."
msgid "Annotations sidebar"
msgstr "Barra lateral de anotaciones"

msgctxt "The accessible label for the list annotations in the annotations sidebar."
msgid "Annotations list"
msgstr "Lista de anotaciones"

msgctxt "The accessible label for the list of thread comments in an annotation."
msgid "Thread comments"
msgstr "Comentarios del hilo"

msgctxt "The accessible label describing the context of the comment thread (e.g. editor content being discussed)."
msgid "THREAD_CONTEXT_LABEL"
msgstr "Contexto del hilo: %0"

msgctxt "The accessible label describing the comment thread."
msgid "COMMENT_THREAD_LABEL"
msgid_plural "Comment thread by %1. %0 replies."
msgstr[0] "Hilo de comentarios de %1. %1 respuesta."
msgstr[1] "Hilo de comentarios de %1. %1 respuesta."

msgctxt "The accessible label providing extra information about navigation in the comment thread."
msgid "COMMENT_THREAD_DESCRIPTION"
msgstr "Pulse \"Intro\" para más opciones. Pulse \"Escape\" para salir."

msgctxt "The accessible label for a single comment."
msgid "COMMENT_ARIA_LABEL"
msgstr "Comentario de %0. Creado el %1. %2. %3"

msgctxt "The accessible label for the list of archived comments."
msgid "COMMENTS_ARCHIVE_LIST_LABEL"
msgstr "Lista de comentarios"

msgctxt "The accessibility help dialog description of the group of keystrokes related to annotations."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_GROUP"
msgstr "Pulsaciones de teclas para interactuar con los hilos de anotación (por ejemplo, comentarios, sugerencias de seguimiento de cambios)"

msgctxt "The accessibility help dialog keystroke description for moving from the edited content to the annotation."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_MOVE_TO"
msgstr "Mover el foco al hilo cuando la selección esté anclada en su marcador"

msgctxt "The accessibility help dialog keystroke description for moving away from an annotation."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_EXIT"
msgstr "Salir de la anotación y volver a centrarse en el contenido editado."

msgctxt "The accessibility help dialog keystroke description for entering a thread."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_ENTER"
msgstr "Explorar el hilo de anotación enfocado o el comentario del hilo"

msgctxt "The accessibility help dialog keystroke description for moving across the internals of a thread."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_BROWSE_GROUPS"
msgstr "Moverse por las partes internas del hilo de anotación"

msgctxt "The accessibility help dialog keystroke description for submitting a comment."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_SUBMIT_COMMENT"
msgstr "Enviar la respuesta mientras se escribe un comentario."

msgctxt "The accessibility help dialog keystroke description for navigating between annotations (threads)."
msgid "ACCESSIBILITY_HELP_ANNOTATIONS_NAVIGATE_THREADS"
msgstr "Desplazarse al hilo anterior o siguiente en la barra lateral de anotaciones o en el archivo de comentarios"

msgctxt "A message read out by assistive technologies the moment the user selection moves into a comment."
msgid "ENTER_COMMENT_ANNOUNCEMENT"
msgstr "Introduzca un contenido con comentarios. Pulse %0 para ir a la discusión."

msgctxt "A message read out by assistive technologies the moment the user selection leaves a comment."
msgid "LEAVE_COMMENT_ANNOUNCEMENT"
msgstr "Dejando un contenido con comentarios."

msgctxt "The label for an alert shown when closing the editor with an unsaved comment change."
msgid "PENDING_ACTION_COMMENT_THREAD"
msgstr "Cambio no guardado en hilo de comentarios."

msgctxt "The additional information that comments comes from external source."
msgid "EXTERNAL_COMMENT"
msgstr "Este comentario procede de una fuente externa."

msgctxt "The additional information that comments comes from imported Word file."
msgid "EXTERNAL_IMPORT_WORD_COMMENT"
msgstr "Este comentario proviene de un archivo Word importado."

msgctxt "The additional information that displayed author name comes from an external source."
msgid "EXTERNAL_AVATAR"
msgstr "El nombre de autor que se muestra proviene de una fuente externa (agregado por %0 )"

msgctxt "The additional information that displayed author name comes from imported Word file."
msgid "EXTERNAL_IMPORT_WORD_AVATAR"
msgstr "El nombre de autor que se muestra proviene de un documento de Word importado por %0"

msgctxt "The label for the comments archive dropdown UI."
msgid "COMMENTS_ARCHIVE"
msgstr "Archivo de comentarios"

msgctxt "The information that comments archive is empty."
msgid "EMPTY_COMMENTS_ARCHIVE"
msgstr "No hay hilos de comentarios archivados."
