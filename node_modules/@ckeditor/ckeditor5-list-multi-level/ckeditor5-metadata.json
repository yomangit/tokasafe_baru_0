{
	"plugins": [
		{
			"name": "Multi-level list",
			"className": "MultiLevelList",
			"description": "Multi-level lists allow the user to set different markers (symbols, text or numbers) to display at each level of the list.",
			"docs": "features/lists/multi-level-lists.html",
			"path": "src/multilevellist.js",
			"requires": [
				"List"
			],
			"uiComponents": [
				{
					"type": "Button",
					"name": "multiLevelList",
					"label": "Multi-level List",
					"iconPath": "theme/icons/multi-level-list.svg"
				}
			],
			"htmlOutput": [
				{
					"elements": [
						"ol",
						"ul"
					],
					"attributes": [
						"start"
					],
					"classes": [ "multi-level-list", "legal-list" ],
					"styles": "list-style-type"
				},
				{
					"elements": "li"
				},
				{
					"elements": "span",
					"classes": "multi-level-list__marker"
				}
			]
		}
	]
}
